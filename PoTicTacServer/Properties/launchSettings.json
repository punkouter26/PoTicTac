{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "http://localhost:5000",
      "applicationUrl": "http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_LOGGING__CONSOLE__INCLUDESCOPES": "true",
        "ASPNETCORE_LOGGING__LOGLEVEL__DEFAULT": "Debug",
        "ASPNETCORE_LOGGING__LOGLEVEL__MICROSOFT": "Information",
        "ASPNETCORE_LOGGING__LOGLEVEL__MICROSOFT.HOSTING.LIFETIME": "Information",
        "ASPNETCORE_DETAILEDERRORS": "true",
        "ASPNETCORE_SHUTDOWNTIMEOUTSECONDS": "30"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "https://localhost:5001",
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_LOGGING__CONSOLE__INCLUDESCOPES": "true",
        "ASPNETCORE_LOGGING__LOGLEVEL__DEFAULT": "Debug",
        "ASPNETCORE_LOGGING__LOGLEVEL__MICROSOFT": "Information",
        "ASPNETCORE_LOGGING__LOGLEVEL__MICROSOFT.HOSTING.LIFETIME": "Information",
        "ASPNETCORE_DETAILEDERRORS": "true",
        "ASPNETCORE_SHUTDOWNTIMEOUTSECONDS": "30"
      }
    }
  }
}
